<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zafiproject.learningfirebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".authentifikasi.RegisterActivity" />
        <activity android:name=".authentifikasi.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".authentifikasi.LoginPhoneActivity" />
        <activity android:name=".authentifikasi.LoginGoogleActivity" />
        <activity android:name=".database.DatabaseActivity" />
        <activity android:name=".admob.AdmobActivity" />
        <activity android:name=".admob.BannerActivity" />
        <activity android:name=".admob.IntertitialActivity" />
        <activity android:name=".admob.RewardVideoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>